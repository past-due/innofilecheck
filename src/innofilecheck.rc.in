1 TYPELIB "@PROJECT_NAME@.rc"

1 VERSIONINFO
FILEVERSION @VERINFO_PRODUCTVERSION@
PRODUCTVERSION @VERINFO_PRODUCTVERSION@
FILEFLAGSMASK 0x3fL
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
FILEOS 0x4L
FILETYPE 0x2L
FILESUBTYPE 0x0L

BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904B0"
        BEGIN
            VALUE "CompanyName",      ""
            VALUE "FileDescription",  "InnoFileCheck Library"
            VALUE "FileVersion",      "@VERINFO_PRODUCTVERSION_STR@"
            VALUE "InternalName",     "innofilecheck"
            VALUE "LegalCopyright",   "Copyright (c) 2018-2025 past-due -- https://github.com/past-due"
            VALUE "OriginalFilename", "innofilecheck.dll"
            VALUE "ProductName",      "InnoFileCheck Library"
            VALUE "ProductVersion",   "@VERINFO_PRODUCTVERSION_STR@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 0x04B0
    END
END
